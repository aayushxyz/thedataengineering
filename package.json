{
  "name": "thedataengineering",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && yarn build:pagefind",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "build:pagefind": "pagefind --site .next/server/app --output-path public/_pagefind",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "next lint",
    "stylelint": "stylelint '**/*.css' --cache",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@gfazioli/mantine-marquee": "^2.2.5",
    "@mantine/carousel": "^7.17.5",
    "@mantine/core": "7.17.2",
    "@mantine/hooks": "7.17.2",
    "@next/bundle-analyzer": "^15.2.4",
    "@tabler/icons-react": "^3.31.0",
    "embla-carousel-react": "^8.6.0",
    "geist": "^1.3.1",
    "next": "15.2.4",
    "nextra": "^4.2.16",
    "nextra-theme-docs": "^4.2.16",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@cloudflare/next-on-pages": "^1.13.12",
    "@eslint/js": "^9.23.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/nextjs": "^8.6.9",
    "@storybook/react": "^8.6.9",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/node": "^22.13.13",
    "@types/react": "19.0.12",
    "babel-loader": "^10.0.0",
    "eslint": "^9.23.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "pagefind": "^1.3.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.3",
    "storybook": "^8.6.9",
    "storybook-dark-mode": "^4.0.2",
    "stylelint": "^16.16.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "ts-jest": "^29.3.0",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.28.0",
    "wrangler": "^4.19.1"
  },
  "packageManager": "yarn@4.7.0"
}
